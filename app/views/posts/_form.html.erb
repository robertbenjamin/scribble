<%= form_for(@post) do |f| %>

  <%= f.label :author %>
  <%= f.text_field :author %>

  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :text %>
  <%= f.text_area :text %>

  <%= f.label :date %>
  <%= f.text_field :date %>

  <%= f.label :photo_url %>
  <%= f.text_field :photo_url %>

  <%= f.submit %>

<% end %>
